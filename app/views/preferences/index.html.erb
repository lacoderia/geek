<h1>Listing preferences</h1>

<table>
  <thead>
    <tr>
      <th>Monday</th>
      <th>Tuesday</th>
      <th>Wednesday</th>
      <th>Thursday</th>
      <th>Friday</th>
      <th>Saturday</th>
      <th>Sunday</th>
      <th>Online</th>
      <th>Only office</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @preferences.each do |preference| %>
      <tr>
        <td><%= preference.monday %></td>
        <td><%= preference.tuesday %></td>
        <td><%= preference.wednesday %></td>
        <td><%= preference.thursday %></td>
        <td><%= preference.friday %></td>
        <td><%= preference.saturday %></td>
        <td><%= preference.sunday %></td>
        <td><%= preference.online %></td>
        <td><%= preference.only_office %></td>
        <td><%= link_to 'Show', preference %></td>
        <td><%= link_to 'Edit', edit_preference_path(preference) %></td>
        <td><%= link_to 'Destroy', preference, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Preference', new_preference_path %>
