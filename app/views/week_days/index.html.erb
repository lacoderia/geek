<h1>Listing week_days</h1>

<table>
  <thead>
    <tr>
      <th>Day</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @week_days.each do |week_day| %>
      <tr>
        <td><%= week_day.day %></td>
        <td><%= link_to 'Show', week_day %></td>
        <td><%= link_to 'Edit', edit_week_day_path(week_day) %></td>
        <td><%= link_to 'Destroy', week_day, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Week day', new_week_day_path %>
