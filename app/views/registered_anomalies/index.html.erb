<h1>Listing registered_anomalies</h1>

<table>
  <thead>
    <tr>
      <th>Anomaly</th>
      <th>User</th>
      <th>Appointment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registered_anomalies.each do |registered_anomaly| %>
      <tr>
        <td><%= registered_anomaly.anomaly_id %></td>
        <td><%= registered_anomaly.user_id %></td>
        <td><%= registered_anomaly.appointment_id %></td>
        <td><%= link_to 'Show', registered_anomaly %></td>
        <td><%= link_to 'Edit', edit_registered_anomaly_path(registered_anomaly) %></td>
        <td><%= link_to 'Destroy', registered_anomaly, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registered anomaly', new_registered_anomaly_path %>
