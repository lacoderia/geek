<% content_for :head_style do %>
<% end %>

<% content_for :head_script do %>
<% end %>

<section ui-view></section>
<% if @user %>
  <div id="user-data" data-email="<%= @user.email %>" data-first-name="<%= @user.first_name %>" data-last-name="<%= @user.last_name %>" data-uid="<%= @user.uid %>" data-token="<%= @user.token %>" data-refresh-token="<%= @user.refresh_token %>"></div>
<% end %>

<% if @login %>
  <div id="login-data" data-error="Usuario o contraseña incorrecta."></div>
<% end %>

<% if @error %>
  <div id="error-data" data-error="Tutor desactivado. Favor de ponerse en contacto con el administrador para más información."></div>
<% end %>
